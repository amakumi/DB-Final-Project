<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="480.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.231" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fp_ui.CustomerController">
    <children>
        <TableView fx:id="tableCustomer" layoutX="220.0" layoutY="28.0" prefHeight="289.0" prefWidth="520.0">
            <columns>
                <TableColumn fx:id="customer_id" prefWidth="60" text="Id" />
                <TableColumn fx:id="customer_name" prefWidth="120.0" text="Customer Name" />
                <TableColumn fx:id="customer_gender" prefWidth="60.0" text="Gender" />
                <TableColumn fx:id="customer_address" prefWidth="100.0" text="Address" />
                <TableColumn fx:id="customer_telp" prefWidth="100.0" text="Telephone" />
                <TableColumn fx:id="customer_dob" prefWidth="80.0" text="DOB" />
            </columns>
        </TableView>
        <Button fx:id="refresh" layoutX="440.0" layoutY="324.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="80.0" text="Refresh" />
        <Button fx:id="insertData" layoutX="100.0" layoutY="324.0" mnemonicParsing="false" onAction="#insertData" prefHeight="43.0" prefWidth="100.0" text="Insert">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
        <Button fx:id="updateData" layoutX="220.0" layoutY="324.0" mnemonicParsing="false" onAction="#updateData" prefHeight="25.0" prefWidth="80.0" text="Update">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
        <Button fx:id="deleteData" layoutX="660.0" layoutY="324.0" mnemonicParsing="false" onAction="#deleteData" prefHeight="25.0" prefWidth="80.0" text="Delete">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
        <TextField fx:id="customerId" layoutX="100.0" layoutY="97.0" prefHeight="35.0" prefWidth="100.0" />
        <TextField fx:id="customerName" layoutX="100.0" layoutY="153.0" prefHeight="25.0" prefWidth="100.0" />
        <TextField fx:id="address" layoutX="100.0" layoutY="221.0" prefHeight="25.0" prefWidth="100.0" />
        <TextField fx:id="telephone" layoutX="100.0" layoutY="255.0" prefHeight="25.0" prefWidth="100.0" />
        <TextField fx:id="dob" layoutX="100.0" layoutY="290.0" prefHeight="25.0" prefWidth="100.0" />
        <Text layoutX="15.0" layoutY="119.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer ID:">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Text>
        <Text layoutX="21.0" layoutY="170.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cust Name:">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Text>
        <Text layoutX="42.0" layoutY="203.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Gender:">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Text>
        <Text layoutX="40.0" layoutY="238.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Address:">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Text>
        <Text layoutX="29.0" layoutY="268.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Telephone:">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Text>
        <Text layoutX="60.0" layoutY="307.0" strokeType="OUTSIDE" strokeWidth="0.0" text="DOB:">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Text>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="441.0" mnemonicParsing="false" onMouseClicked="#backButtonAction" text="BACK">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
      <Label layoutX="39.0" layoutY="28.0" text="CUSTOMER" textFill="#5900ff">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <ComboBox fx:id="gender" layoutX="100.0" layoutY="186.0" prefHeight="25.0" prefWidth="100.0" />
    </children>
</AnchorPane>
