<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" fx:id="insert" prefHeight="494.0" prefWidth="718.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="fp_ui.TransactionController">
    <children>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69"/>
        <TableView fx:id="tableTransaction" layoutX="223.0" layoutY="15.0" prefHeight="289.0" prefWidth="469.0">
            <columns>
                <TableColumn fx:id="transaction_id" prefWidth="160.0" text="Id"/>
                <TableColumn fx:id="customer_id" prefWidth="105.66668701171875" text="Customer Id"/>
                <TableColumn fx:id="staff_id" prefWidth="124.33331298828125" text="Staff Id"/>
            </columns>
        </TableView>
        <Button fx:id="btn_generate" layoutX="571.0" layoutY="317.0" mnemonicParsing="false"
                onAction="#handleButtonAction" prefHeight="32.0" prefWidth="121.0" text="Refresh"/>
        <Button fx:id="insertData" layoutX="103.0" layoutY="238.0" mnemonicParsing="false" onAction="#insertData"
                prefHeight="66.0" prefWidth="112.0" text="Insert">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Button>
        <Button fx:id="deleteData" layoutX="571.0" layoutY="363.0" mnemonicParsing="false" onAction="#deleteData"
                prefHeight="32.0" prefWidth="121.0" text="Delete">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Button>
        <TextField fx:id="id_1" layoutX="103.0" layoutY="108.0" prefHeight="25.0" prefWidth="112.0"/>
        <Text layoutX="14.0" layoutY="125.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Transaction ID:"
              wrappingWidth="89.01693725585938">
            <font>
                <Font name="System Bold Italic" size="12.0"/>
            </font>
        </Text>
        <TextField fx:id="id" layoutX="103.0" layoutY="155.0" prefHeight="25.0" prefWidth="112.0"/>
        <Text layoutX="25.0" layoutY="172.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer ID:">
            <font>
                <Font name="System Bold Italic" size="12.0"/>
            </font>
        </Text>
        <Button fx:id="updateData" layoutX="223.0" layoutY="317.0" mnemonicParsing="false" onAction="#updateData"
                prefHeight="35.0" prefWidth="121.0" text="Update"/>
        <TextField fx:id="id_2" layoutX="103.0" layoutY="201.0" prefHeight="25.0" prefWidth="112.0"/>
        <Text layoutX="52.0" layoutY="218.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Staff ID:"
              wrappingWidth="57.333343505859375">
            <font>
                <Font name="System Bold Italic" size="12.0"/>
            </font>
        </Text>
        <Button fx:id="btnBack" layoutX="14.0" layoutY="455.0" mnemonicParsing="false"
                onMouseClicked="#backButtonAction" text="BACK">
            <font>
                <Font name="System Bold Italic" size="12.0"/>
            </font>
        </Button>
        <Label layoutX="24.0" layoutY="34.0" text="TRANSACTION" textFill="#5e00ff">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
